class Base {
  //Atributos
  char letra;
  int valor;
  //Constructor
  Base() {
    //Agregar numeros
    valor = int(random(47,122));
    //valor = int(random(63, 122));
    //while (valor > 90 && valor < 97) {
  while ((valor>57 && valor <63) ||(valor>90 && valor< 97)){   
      valor = int(random(65, 122));
    }
    if (valor == 63) {
      valor = 32;
    }
    if (valor == 64) {
      valor = 46;
    }
    letra = (char) valor;
    }
  
  //MÃ©todos
  void showData(){
    print(letra);
  }
}
